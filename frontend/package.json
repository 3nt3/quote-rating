{
	"name": "smui-example-sveltekit",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@material/theme": "^14.0.0",
		"@material/typography": "^14.0.0",
		"@mdi/js": "^7.0.96",
		"@smui/button": "^7.0.0-beta.0",
		"@smui/card": "^7.0.0-beta.0",
		"@smui/circular-progress": "^7.0.0-beta.0",
		"@smui/icon-button": "^7.0.0-beta.0",
		"@smui/layout-grid": "^7.0.0-beta.0",
		"@smui/top-app-bar": "^7.0.0-beta.0",
		"@sveltejs/adapter-auto": "^1.0.0-next.88",
		"@sveltejs/adapter-node": "^1.0.0-next.101",
		"@sveltejs/kit": "^1.0.0-next.551",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"smui-theme": "^7.0.0-beta.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.2.2"
	},
	"type": "module",
	"dependencies": {
		"chart.js": "^4.0.1",
		"svelte-chartjs": "^3.1.0"
	}
}
